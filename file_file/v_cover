function v_cover(Graph g)
{
    propNode<bool> visited;
    g.attachNodeProperty(visited = False);
    forall(v in g.nodes().filter(visited == False)){
        for(nbr in g.neighbors(v)){
            if(nbr.visited == False){
                nbr.visited = True;
                v.visited = True;
            }
        }
    }
}